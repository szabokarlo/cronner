{
    "name": "szabokarlo/cronner",
    "description": "Application for csv import cronner",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Károly Szabó",
            "email": "szabokarlo@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6",
        "ext-PDO": "^7.4",
        "php-di/php-di": "^5.4",
        "webmozart/assert": "^1.9",
        "league/csv": "^8.2",
        "ext-mbstring": "^7.4",
        "ext-iconv": "^7.4",
        "monolog/monolog": "^1.25"
    },
    "config": {
        "platform": {
            "php": "5.6.0"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Cronner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
